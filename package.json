{
	"name": "openapi-axios-generator",
	"version": "1.0.0",
	"description": "Generate Axios API clients from OpenAPI specifications",
	"main": "dist/index.js",
	"bin": {
		"generate-api": "./dist/cli.js"
	},
	"scripts": {
		"build": "tsc",
		"start": "node dist/cli.js",
		"test": "tsc -p test/tsconfig.json && node dist/test/index.js",
		"check": "biome check --write .",
		"updates": "npx npm-check-updates -i"
	},
	"dependencies": {
		"axios": "^1.7.9",
		"openapi-types": "^12.1.3",
		"yaml": "^2.7.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@tanstack/react-query": "^5.66.9",
		"@types/node": "^22.13.5",
		"typescript": "^5.7.3"
	},
	"peerDependencies": {
		"@tanstack/react-query": "^5.0.0"
	}
}
